{
    "name": "joomla/uri",
    "type": "joomla-package",
    "description": "Joomla Uri Package",
    "keywords": ["joomla", "framework", "uri"],
    "homepage": "https://github.com/joomla-framework/uri",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "~7.0"
    },
    "require-dev": {
        "joomla/coding-standards": "~2.0@alpha",
	    "joomla/test": "~1.0",
        "phpunit/phpunit": "~6.3"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Uri\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Uri\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
	        "dev-2.0-dev": "2.0-dev"
        }
    }
}
